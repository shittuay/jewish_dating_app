    # This file is an example of the environment variables needed for this project.
    # Create a copy of this file named .env and fill in the actual values.

    # DeepSeek API Configuration
    DEEPSEEK_API_KEY=JEWISH_DATING_APP

    # Flask Configuration
    FLASK_APP=app.py
    FLASK_ENV=development
    SECRET_KEY=your_secret_key

    # Database Configuration
    DATABASE_URL=sqlite:///instance/jewish_dating.sqlite

    # Email Configuration
    MAIL_SERVER=smtp.gmail.com
    MAIL_PORT=587
    MAIL_USE_TLS=True
    MAIL_USERNAME=your-email@gmail.com
    MAIL_PASSWORD=your-app-password
    MAIL_DEFAULT_SENDER="Jewish Dating App <your-email@gmail.com>"
